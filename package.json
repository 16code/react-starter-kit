{
    "name": "react-starter-kit",
    "version": "1.0.0",
    "main": "app.js",
    "author": "StarLau",
    "license": "MIT",
    "scripts": {
        "dev": "cross-env BABEL_ENV=development NODE_ENV=development npx webpack-dev-server --hot",
        "prebuild:prod": "rm -rf dist.zip",
        "prebuild:test": "rm -rf dist.zip",
        "preview": "cross-env NODE_ENV=development node server.js",
        "build:prod": "cross-env BABEL_ENV=production NODE_ENV=production npx webpack -p --progress --colors --display minimal --env.prod",
        "build:test": "cross-env BABEL_ENV=production NODE_ENV=production npx webpack -p --progress --colors --display minimal --env.test",
        "lint:js": "npx eslint --cache --cache-location '.cache/eslintcache' --quiet './src/**/*.js' './src/**/*.jsx'",
        "lint:css": "npx stylelint --cache --cache-location '.cache/stylelintcache' 'src/**/*.css' 'src/**/*.less' --syntax less --fix"
    },
    "dependencies": {
        "antd": "^3.10.4",
        "classnames": "^2.2.6",
        "enquire.js": "^2.1.6",
        "lodash": "^4.17.11",
        "moment": "^2.22.2",
        "react": "^16.6.1",
        "react-container-query": "^0.11.0",
        "react-document-title": "^2.0.3",
        "react-dom": "^16.6.1",
        "react-redux": "^5.1.0",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.1",
        "redux-saga": "^0.16.2",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.1.5",
        "@babel/plugin-proposal-class-properties": "^7.1.0",
        "@babel/plugin-proposal-decorators": "^7.1.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-react-constant-elements": "^7.0.0",
        "@babel/plugin-transform-react-inline-elements": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/preset-env": "^7.1.5",
        "@babel/preset-react": "^7.0.0",
        "@babel/runtime": "^7.1.5",
        "add-asset-html-webpack-plugin": "^3.1.2",
        "autoprefixer": "^9.3.1",
        "babel-eslint": "^10.0.1",
        "babel-loader": "^8.0.4",
        "babel-plugin-import": "^1.11.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.20",
        "cache-loader": "^1.2.5",
        "clean-webpack-plugin": "^0.1.19",
        "compression-webpack-plugin": "^2.0.0",
        "copy-webpack-plugin": "^4.6.0",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.1",
        "cssnano": "^4.1.7",
        "eslint": "^5.8.0",
        "eslint-config-prettier": "^3.1.0",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-prettier": "^3.0.0",
        "eslint-plugin-react": "^7.11.1",
        "express": "^4.16.4",
        "express-http-proxy": "^1.5.0",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "less": "^3.8.1",
        "less-loader": "^4.1.0",
        "mini-css-extract-plugin": "^0.4.4",
        "path": "^0.12.7",
        "postcss-css-variables": "^0.11.0",
        "postcss-loader": "^3.0.0",
        "postcss-short": "^5.0.0",
        "pre-commit": "^1.2.2",
        "prop-types": "^15.6.2",
        "react-hot-loader": "^4.3.12",
        "redux-logger": "^3.0.6",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.1",
        "stylelint": "^9.7.1",
        "stylelint-config-prettier": "^4.0.0",
        "stylelint-config-recommended": "^2.1.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-webpack-plugin": "^0.10.5",
        "uglifyjs-webpack-plugin": "^2.0.1",
        "url-loader": "^1.1.2",
        "webpack": "^4.25.1",
        "webpack-bundle-analyzer": "^3.0.3",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.10"
    },
    "precommit": {
        "run": [
            "lint:js",
            "lint:css"
        ],
        "silent": true
    }
}
