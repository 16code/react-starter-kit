{
    "name": "react-starter-kit",
    "version": "1.0.0",
    "main": "app.js",
    "author": "StarLau",
    "license": "MIT",
    "scripts": {
        "dev": "cross-env BABEL_ENV=development NODE_ENV=development npx webpack-dev-server --hot",
        "prebuild:prod": "rm -rf dist.zip",
        "prebuild:test": "rm -rf dist.zip",
        "preview": "cross-env NODE_ENV=development node server.js",
        "build:prod": "cross-env BABEL_ENV=production NODE_ENV=production npx webpack -p --progress --colors --display minimal --env.prod",
        "build:test": "cross-env BABEL_ENV=production NODE_ENV=production npx webpack -p --progress --colors --display minimal --env.test",
        "lint:js": "npx eslint --cache --cache-location '.cache/eslintcache' --quiet './src/**/*.js' './src/**/*.jsx'",
        "lint:css": "npx stylelint --cache --cache-location '.cache/stylelintcache' 'src/**/*.css' 'src/**/*.less' --syntax less --fix"
    },
    "dependencies": {
        "antd": "^3.9.2",
        "classnames": "^2.2.6",
        "enquire.js": "^2.1.6",
        "lodash": "^4.17.11",
        "moment": "^2.22.2",
        "react": "^16.5.1",
        "react-container-query": "^0.11.0",
        "react-document-title": "^2.0.3",
        "react-dom": "^16.5.1",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.3.1",
        "redux": "^4.0.0",
        "redux-saga": "^0.16.0",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
		"@babel/runtime": "^7.0.0",
        "@babel/core": "^7.0.0",
        "@babel/plugin-proposal-class-properties": "^7.0.0",
        "@babel/plugin-proposal-decorators": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-react-constant-elements": "^7.0.0",
        "@babel/plugin-transform-react-inline-elements": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "add-asset-html-webpack-plugin": "^3.0.0",
        "autoprefixer": "^9.1.5",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.0.0",
        "babel-plugin-import": "^1.9.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
        "cache-loader": "^1.2.2",
        "clean-webpack-plugin": "^0.1.19",
        "compression-webpack-plugin": "^2.0.0",
        "copy-webpack-plugin": "^4.5.2",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "cssnano": "^4.1.0",
        "eslint": "^5.6.0",
        "eslint-config-prettier": "^3.0.1",
        "eslint-loader": "^2.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-prettier": "^2.6.2",
        "eslint-plugin-react": "^7.11.1",
        "express": "^4.16.3",
        "express-http-proxy": "^1.2.0",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "less": "^3.8.1",
        "less-loader": "^4.1.0",
        "mini-css-extract-plugin": "^0.4.2",
        "path": "^0.12.7",
        "postcss-css-variables": "^0.9.0",
        "postcss-loader": "^3.0.0",
        "postcss-short": "^4.1.0",
        "pre-commit": "^1.2.2",
        "prop-types": "^15.6.2",
        "react-hot-loader": "^4.3.8",
        "redux-logger": "^3.0.6",
        "source-map-loader": "^0.2.4",
        "style-loader": "^0.23.0",
        "stylelint": "^9.5.0",
        "stylelint-config-prettier": "^4.0.0",
        "stylelint-config-recommended": "^2.1.0",
        "stylelint-config-standard": "^18.2.0",
        "stylelint-webpack-plugin": "^0.10.5",
        "uglifyjs-webpack-plugin": "^2.0.0",
        "url-loader": "^1.1.1",
        "webpack": "^4.19.0",
        "webpack-bundle-analyzer": "^3.0.2",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.8",
        "why-did-you-update": "^0.1.1"
    },
    "precommit": {
        "run": [
            "lint:js",
            "lint:css"
        ],
        "silent": true
    }
}
