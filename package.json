        "redux": "^4.0.0",
        "redux-saga": "^0.16.0"
    },
    "devDependencies": {
        "add-asset-html-webpack-plugin": "^2.1.3",
        "autoprefixer": "^9.0.1",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.6",
        "babel-loader": "^7.1.5",
